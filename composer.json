{
    "name": "linemob/core",
    "type": "library",
    "description": "Line Bot Translation using google translation api.",
    "keywords": [
        "line",
        "bot"
    ],
    "homepage": "https://github.com/linemob",
    "license": "MIT",
    "authors": [
        {
            "name": "liverbool",
            "homepage": "http://github.com/liverbool"
        }
    ],
    "autoload": {
        "psr-4": {
            "LineMob\\Core\\": "src/"
        },
        "classmap": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LineMob\\Core\\spec\\": "spec/"
        }
    },
    "require": {
        "php": ">=5.6",
        "league/tactician-bundle": "~0.4",
        "linecorp/line-bot-sdk": "^2.1",

        "psr/http-message": "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/httplug": "^1.0",
        "php-http/discovery": "^1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.0",
        "php-http/guzzle6-adapter": "^1.1"
    },
    "config": {
        "bin-dir": "bin"
    }
}
